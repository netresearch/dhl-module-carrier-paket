<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminShipmentPaketPackagingPopupSection">
        <element name="createShippingLabel" type="button" selector="[data-ui-id='page-actions-toolbar-save-button']"/>
        <element name="packageItems" type="text" selector=".dhl-fieldset.items"/>
        <element name="packageOptions" type="text" selector=".dhl-fieldset.package"/>
        <element name="serviceOptions" type="text" selector=".dhl-fieldset.service"/>
        <element name="shippingProduct" type="text" selector=".dhl-fieldset.package .productCode .admin__field label span"/>
        <element name="totalWeight" type="input" selector=".dhl-fieldset.package .field.weight input.weight"/>
        <element name="customsValue" type="input" selector=".dhl-fieldset.package .field.customsValue input.customsValue"/>
        <element name="termsOfTrade" type="select" selector=".dhl-fieldset.package .field.termsOfTrade select"/>
        <element name="exportContentType" type="select" selector=".dhl-fieldset.package .field.contentType select"/>
        <element name="explanation" type="input" selector=".dhl-fieldset.package .field.explanation input.explanation"/>
        <element name="packageDescription" type="textarea" selector=".dhl-fieldset.package .field.exportDescription textarea"/>
        <element name="additionalFee" type="input" selector=".dhl-fieldset.package .field.additionalFee input.additionalFee"/>
        <element name="placeOfCommittal" type="input" selector=".dhl-fieldset.package .field.placeOfCommittal input.placeOfCommittal"/>
        <element name="permitNumber" type="input" selector=".dhl-fieldset.package .field.permitNumber input.permitNumber"/>
        <element name="attestationNumber" type="input" selector=".dhl-fieldset.package .field.attestationNumber input.attestationNumber"/>
        <element name="electronicExportNotification" type="checkbox" selector=".dhl-fieldset.package .field.electronicExportNotification input"/>
     </section>
</sections>
