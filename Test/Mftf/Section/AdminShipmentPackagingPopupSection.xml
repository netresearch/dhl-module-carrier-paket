<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <!-- Extend section with DHL Paket specific service and package data-->
    <section name="AdminShipmentPackagingPopupSection">
        <!-- Package level selectors -->
        <element name="customsFees" type="input" selector=".nrshipping-fieldset.package .field.customsFees input.customsFees"/>
        <element name="placeOfCommittal" type="input" selector=".nrshipping-fieldset.package .field.placeOfCommittal input.placeOfCommittal"/>
        <element name="permitNumber" type="input" selector=".nrshipping-fieldset.package .field.permitNumber input.permitNumber"/>
        <element name="attestationNumber" type="input" selector=".nrshipping-fieldset.package .field.attestationNumber input.attestationNumber"/>
        <element name="electronicExportNotification" type="checkbox" selector=".nrshipping-fieldset.package .field.electronicExportNotification input"/>
        <!-- Service level selectors -->
     </section>
</sections>
