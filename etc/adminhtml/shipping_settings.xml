<?xml version="1.0" ?>
<carriers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Dhl_ShippingCore/etc/shipping_settings.xsd">
    <carrier code="dhlpaket">
        <packageOptions>
            <shippingOption code="packageCustoms">
                <inputs>
                    <input code="additionalFee" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/additional_fee">
                        <label>Additional Fee</label>
                        <inputType>prefixed</inputType>
                    </input>
                    <input code="placeOfCommittal" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/place_of_committal">
                        <label>Place of Committal</label>
                        <inputType>text</inputType>
                    </input>
                    <input code="permitNumber">
                        <label>Permit Number</label>
                        <inputType>text</inputType>
                    </input>
                    <input code="attestationNumber">
                        <label>Attestation Number</label>
                        <inputType>text</inputType>
                    </input>
                    <input code="electronicExportNotification" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/electronic_export_notification">
                        <label>Electronic Export Notification</label>
                        <inputType>checkbox</inputType>
                    </input>
                </inputs>
            </shippingOption>
        </packageOptions>
        <itemOptions>
            <itemShippingOptions itemId="0">
                <shippingOptions>
                    <shippingOption code="itemCustoms">
                        <routes>
                            <route>
                                <excludeDestinations>
                                    <destination>eu</destination>
                                </excludeDestinations>
                            </route>
                        </routes>
                    </shippingOption>
                </shippingOptions>
            </itemShippingOptions>
        </itemOptions>
        <serviceOptions>
            <shippingOption code="parcelshopFinder">
                <sortOrder>500</sortOrder>
                <label>Parcel Station</label>
                <inputs>
                    <input code="enabled">
                        <label>Parcel Station Delivery</label>
                    </input>
                    <input code="customerPostnumber">
                        <inputType>static</inputType>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="parcelAnnouncement">
                <sortOrder>501</sortOrder>
                <inputs>
                    <input code="enabled">
                        <disabled>true</disabled>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="printOnlyIfCodeable">
                <label>Routing Code</label>
                <sortOrder>502</sortOrder>
                <inputs>
                    <input code="enabled" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/print_only_if_codeable">
                        <inputType>checkbox</inputType>
                        <label>Enable DHL print only if codeable service</label>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="returnShipment">
                <sortOrder>503</sortOrder>
                <label>Return Shipment</label>
                <inputs>
                    <input code="enabled" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/services_group/return_shipment">
                        <inputType>checkbox</inputType>
                        <label>Enable DHL return shipment service</label>
                    </input>
                </inputs>
                <routes>
                    <route>
                        <origin>DE</origin>
                        <includeDestinations>
                            <destination>DE</destination>
                        </includeDestinations>
                    </route>
                </routes>
            </shippingOption>
            <shippingOption code="additionalInsurance">
                <sortOrder>504</sortOrder>
                <label>Additional Insurance</label>
                <inputs>
                    <input code="enabled" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/services_group/additional_insurance">
                        <inputType>checkbox</inputType>
                        <label>Enable DHL additional insurance service</label>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="parcelOutletRouting">
                <sortOrder>505</sortOrder>
                <label>Parcel Routing</label>
                <routes>
                    <route>
                        <origin>DE</origin>
                        <includeDestinations>
                            <destination>DE</destination>
                        </includeDestinations>
                    </route>
                </routes>
                <inputs>
                    <input code="enabled" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/services_group/parcel_outlet">
                        <inputType>checkbox</inputType>
                        <label>Enable DHL retail outlet routing service</label>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="bulkyGoods">
                <sortOrder>506</sortOrder>
                <label>Bulky Goods</label>
                <inputs>
                    <input code="enabled" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/services_group/bulky_goods">
                        <inputType>checkbox</inputType>
                        <label>Enable DHL bulky goods service</label>
                    </input>
                </inputs>
                <routes>
                    <route>
                        <origin>DE</origin>
                        <includeDestinations>
                            <destination>DE</destination>
                        </includeDestinations>
                    </route>
                </routes>
            </shippingOption>
            <shippingOption code="visualCheckOfAge">
                <sortOrder>507</sortOrder>
                <label>Visual Check of Age</label>
                <inputs>
                    <input code="enabled" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/services_group/visual_check_of_age">
                        <inputType>radioset</inputType>
                        <options>
                            <option id="no">
                                <label>No</label>
                                <value>0</value>
                            </option>
                            <option id="A16">
                                <label>A16</label>
                                <value>A16</value>
                            </option>
                            <option id="A18">
                                <label>A18</label>
                                <value>A18</value>
                            </option>
                        </options>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="preferredDay">
                <sortOrder>508</sortOrder>
                <label>Preferred Day Delivery</label>
                <inputs>
                    <input code="enabled">
                        <sortOrder>10</sortOrder>
                        <inputType>checkbox</inputType>
                        <label>Enable DHL preferred day delivery</label>
                    </input>
                    <input code="date">
                        <sortOrder>20</sortOrder>
                        <inputType>radio</inputType>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="preferredTime">
                <sortOrder>509</sortOrder>
                <label>Preferred Time Delivery</label>
                <inputs>
                    <input code="enabled">
                        <sortOrder>10</sortOrder>
                        <inputType>checkbox</inputType>
                        <label>Enable DHL preferred time delivery</label>
                    </input>
                    <input code="time">
                        <sortOrder>20</sortOrder>
                        <inputType>radio</inputType>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="preferredNeighbour">
                <sortOrder>510</sortOrder>
                <label>Preferred Neighbour Delivery</label>
                <inputs>
                    <input code="enabled">
                        <sortOrder>10</sortOrder>
                        <inputType>checkbox</inputType>
                        <label>Enable DHL preferred neighbour delivery</label>
                    </input>
                    <input code="name">
                        <sortOrder>20</sortOrder>
                        <inputType>static</inputType>
                    </input>
                    <input code="address">
                        <sortOrder>30</sortOrder>
                        <inputType>static</inputType>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="preferredLocation">
                <sortOrder>511</sortOrder>
                <label>Preferred Location Delivery</label>
                <inputs>
                    <input code="enabled">
                        <sortOrder>10</sortOrder>
                        <inputType>checkbox</inputType>
                        <label>Enable DHL preferred location delivery</label>
                    </input>
                    <input code="details">
                        <sortOrder>20</sortOrder>
                        <inputType>static</inputType>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="cashOnDelivery">
                <inputs>
                    <input code="reasonForPayment" defaultConfigValue="dhlshippingsolutions/dhlpaket/shipment_defaults/cod_reason_for_payment">
                        <label>Reason for Payment</label>
                        <inputType>text</inputType>
                        <validationRules>
                            <rule name="max_text_length">
                                <param>70</param>
                            </rule>
                        </validationRules>
                    </input>
                </inputs>
            </shippingOption>
        </serviceOptions>
        <compatibilityData>
            <rule id="cashOnDeliveryDependency">
                <masters>
                    <master>cashOnDelivery.enabled</master>
                </masters>
                <triggerValue>*</triggerValue>
                <action>show</action>
                <subjects>
                    <subject>cashOnDelivery.reasonForPayment</subject>
                </subjects>
            </rule>
        </compatibilityData>
    </carrier>
</carriers>
