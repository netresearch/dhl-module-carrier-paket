<?xml version="1.0" ?>
<carriers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Dhl_ShippingCore/etc/shipping_settings.xsd">
    <carrier code="dhlpaket">
        <packageOptions>
            <shippingOption code="Dhl\ShippingCore\Model\ShippingSettings\ShippingOption\Codes::PACKAGING_OPTION_PACKAGE_CUSTOMS">
                <inputs>
                    <input code="additionalFee" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_ADDITIONAL_FEE">
                        <label translate="true">Additional Fee</label>
                        <inputType>prefixed</inputType>
                        <sortOrder>1010</sortOrder>
                    </input>
                    <input code="placeOfCommittal" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_PLACE_OF_COMMITTAL">
                        <label translate="true">Place of Committal</label>
                        <inputType>text</inputType>
                        <sortOrder>1020</sortOrder>
                    </input>
                    <input code="permitNumber">
                        <label translate="true">Permit Number</label>
                        <inputType>text</inputType>
                        <sortOrder>1030</sortOrder>
                    </input>
                    <input code="attestationNumber">
                        <label translate="true">Attestation Number</label>
                        <inputType>text</inputType>
                        <sortOrder>1040</sortOrder>
                    </input>
                    <input code="electronicExportNotification" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_ELECTRONIC_EXPORT_NOTIFICATION">
                        <label translate="true">Electronic Export Notification</label>
                        <inputType>checkbox</inputType>
                        <sortOrder>1050</sortOrder>
                    </input>
                </inputs>
            </shippingOption>
        </packageOptions>
        <serviceOptions>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::CHECKOUT_SERVICE_PARCELSHOP_FINDER">
                <label translate="true">Parcel Station</label>
                <sortOrder>1010</sortOrder>
                <inputs>
                    <input code="enabled">
                        <label translate="true">Parcel Station Delivery</label>
                    </input>
                    <input code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::CHECKOUT_INPUT_CUSTOMER_POSTNUMBER">
                        <inputType>static</inputType>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::CHECKOUT_PARCEL_ANNOUNCEMENT">
                <sortOrder>1020</sortOrder>
                <inputs>
                    <input code="enabled">
                        <disabled>true</disabled>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::PACKAGING_PRINT_ONLY_IF_CODEABLE">
                <label translate="true">Routing Code</label>
                <sortOrder>1030</sortOrder>
                <inputs>
                    <input code="enabled" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_PRINT_ONLY_IF_CODEABLE">
                        <inputType>checkbox</inputType>
                        <label translate="true">Enable DHL print only if codeable service</label>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::PACKAGING_SERVICE_RETURN_SHIPMENT">
                <label translate="true">Return Shipment</label>
                <sortOrder>1040</sortOrder>
                <inputs>
                    <input code="enabled" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_RETURN_SHIPMENT">
                        <inputType>checkbox</inputType>
                        <label translate="true">Enable DHL return shipment service</label>
                    </input>
                </inputs>
                <routes>
                    <route>
                        <origin>DE</origin>
                        <includeDestinations>
                            <destination>DE</destination>
                        </includeDestinations>
                    </route>
                </routes>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::PACKAGING_SERVICE_INSURANCE">
                <label translate="true">Additional Insurance</label>
                <sortOrder>1050</sortOrder>
                <inputs>
                    <input code="enabled" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_ADDITIONAL_INSURANCE">
                        <inputType>checkbox</inputType>
                        <label translate="true">Enable DHL additional insurance service</label>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::CHECKOUT_SERVICE_PARCEL_OUTLET_ROUTING">
                <label translate="true">Parcel Routing</label>
                <sortOrder>1060</sortOrder>
                <routes>
                    <route>
                        <origin>DE</origin>
                        <includeDestinations>
                            <destination>DE</destination>
                        </includeDestinations>
                    </route>
                </routes>
                <inputs>
                    <input code="enabled" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_PARCEL_OUTLET">
                        <inputType>checkbox</inputType>
                        <label translate="true">Enable DHL retail outlet routing service</label>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::PACKAGING_SERVICE_BULKY_GOODS">
                <label translate="true">Bulky Goods</label>
                <sortOrder>1070</sortOrder>
                <inputs>
                    <input code="enabled" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_BULKY_GOODS">
                        <inputType>checkbox</inputType>
                        <label translate="true">Enable DHL bulky goods service</label>
                    </input>
                </inputs>
                <routes>
                    <route>
                        <origin>DE</origin>
                        <includeDestinations>
                            <destination>DE</destination>
                        </includeDestinations>
                    </route>
                </routes>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::PACKAGING_SERVICE_CHECK_OF_AGE">
                <label translate="true">Visual Check of Age</label>
                <sortOrder>1080</sortOrder>
                <inputs>
                    <input code="enabled" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_VISUAL_CHECK_OF_AGE">
                        <inputType>radioset</inputType>
                        <options>
                            <option id="no">
                                <label>No</label>
                                <value>0</value>
                            </option>
                            <option id="A16">
                                <label>A16</label>
                                <value>A16</value>
                            </option>
                            <option id="A18">
                                <label>A18</label>
                                <value>A18</value>
                            </option>
                        </options>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::CHECKOUT_SERVICE_PREFERRED_DAY">
                <label translate="true">Preferred Day Delivery</label>
                <sortOrder>1090</sortOrder>
                <inputs>
                    <input code="enabled">
                        <sortOrder>10</sortOrder>
                        <inputType>checkbox</inputType>
                        <label translate="true">Enable DHL preferred day delivery</label>
                    </input>
                    <input code="date">
                        <sortOrder>20</sortOrder>
                        <inputType>radio</inputType>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::CHECKOUT_SERVICE_PREFERRED_NEIGHBOUR">
                <label translate="true">Preferred Neighbour Delivery</label>
                <sortOrder>1110</sortOrder>
                <inputs>
                    <input code="enabled">
                        <sortOrder>10</sortOrder>
                        <inputType>checkbox</inputType>
                        <label translate="true">Enable DHL preferred neighbour delivery</label>
                    </input>
                    <input code="name">
                        <sortOrder>20</sortOrder>
                        <inputType>static</inputType>
                    </input>
                    <input code="address">
                        <sortOrder>30</sortOrder>
                        <inputType>static</inputType>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::CHECKOUT_SERVICE_PREFERRED_LOCATION">
                <label translate="true">Preferred Location Delivery</label>
                <sortOrder>1120</sortOrder>
                <inputs>
                    <input code="enabled">
                        <sortOrder>10</sortOrder>
                        <inputType>checkbox</inputType>
                        <label translate="true">Enable DHL preferred location delivery</label>
                    </input>
                    <input code="details">
                        <sortOrder>20</sortOrder>
                        <inputType>static</inputType>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::PACKAGING_SERVICE_CASH_ON_DELIVERY">
                <label translate="true">Cash on Delivery</label>
                <sortOrder>1010</sortOrder>
                <inputs>
                    <input code="enabled">
                        <label translate="true">Cash on Delivery</label>
                        <inputType>checkbox</inputType>
                        <disabled>true</disabled>
                        <defaultValue>false</defaultValue>
                    </input>
                    <input code="reasonForPayment" defaultConfigValue="Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_COD_REASON_FOR_PAYMENT">
                        <label translate="true">Reason for Payment</label>
                        <inputType>text</inputType>
                        <validationRules>
                            <rule name="max_text_length">
                                <param>70</param>
                            </rule>
                        </validationRules>
                    </input>
                </inputs>
            </shippingOption>
        </serviceOptions>
        <compatibilityData>
            <rule id="cashOnDeliveryDependency">
                <masters>
                    <master>Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::PACKAGING_SERVICE_CASH_ON_DELIVERY.enabled</master>
                </masters>
                <triggerValue>*</triggerValue>
                <action>show</action>
                <subjects>
                    <subject>Dhl\Paket\Model\ShippingSettings\ShippingOption\Codes::PACKAGING_SERVICE_CASH_ON_DELIVERY.reasonForPayment</subject>
                </subjects>
            </rule>
        </compatibilityData>
    </carrier>
</carriers>
