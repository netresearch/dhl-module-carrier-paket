<?xml version="1.0" ?>
<carriers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Dhl_ShippingCore/etc/shipping_options.xsd">
    <carrier code="dhlpaket">
        <serviceOptions>
            <shippingOption code="preferredDay">
                <inputs>
                    <input code="date">
                        <options>
                            <option id="none">
                                <label>none</label>
                            </option>
                        </options>
                    </input>
                </inputs>
            </shippingOption>
            <shippingOption code="preferredTime">
                <inputs>
                    <input code="time">
                        <options>
                            <option id="none">
                                <label>none</label>
                            </option>
                        </options>
                    </input>
                </inputs>
            </shippingOption>
        </serviceOptions>
    </carrier>
</carriers>
