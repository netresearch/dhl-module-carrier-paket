<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <!-- # interface preferences -->
    <preference for="Dhl\Paket\Webservice\Processor\OperationProcessorInterface" type="Dhl\Paket\Webservice\Processor\LabelStatusProcessor"/>
    <preference for="Dhl\Paket\Model\ShippingProducts\ShippingProductsInterface" type="Dhl\Paket\Model\ShippingProducts\ShippingProducts"/>

    <!-- # sdk preferences -->
    <preference for="Dhl\Sdk\Paket\Bcs\Api\ShipmentOrderRequestBuilderInterface" type="Dhl\Sdk\Paket\Bcs\RequestBuilder\ShipmentOrderRequestBuilder"/>
    <preference for="Dhl\Sdk\Paket\Bcs\Api\ServiceFactoryInterface" type="Dhl\Sdk\Paket\Bcs\Service\ServiceFactory"/>
    <preference for="Dhl\Sdk\Paket\ParcelManagement\Api\ServiceFactoryInterface" type="Dhl\Sdk\Paket\ParcelManagement\Service\ServiceFactory"/>
    <preference for="Dhl\Sdk\Paket\Bcs\Api\Data\AuthenticationStorageInterface" type="Dhl\Sdk\Paket\Bcs\Auth\AuthenticationStorage"/>

    <!-- # virtual types -->
    <virtualType name="Dhl\Paket\Model\BulkShipmentManagement\Virtual" type="Dhl\Paket\Model\ShipmentManagement">
        <arguments>
            <argument name="createResponseProcessor" xsi:type="object">Dhl\ShippingCore\Webservice\Pipeline\BulkShipment\ShipmentResponseProcessor\Virtual</argument>
        </arguments>
    </virtualType>
    <virtualType name="Dhl\Paket\Model\Logger\Handler\Virtual" type="Dhl\ShippingCore\Model\Log\ApiLogHandler">
        <arguments>
            <argument name="fileName" xsi:type="string">var/log/dhl_paket.log</argument>
            <argument name="logEnabledConfigPath" xsi:type="const">Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_ENABLE_LOGGING</argument>
            <argument name="logLevelConfigPath" xsi:type="const">Dhl\Paket\Model\Config\ModuleConfig::CONFIG_PATH_LOGLEVEL</argument>
        </arguments>
    </virtualType>
    <virtualType name="Dhl\Paket\Model\Logger\Virtual" type="Monolog\Logger">
        <arguments>
            <argument name="name" xsi:type="const">Dhl\Paket\Model\Carrier\Paket::CARRIER_CODE</argument>
            <argument name="handlers" xsi:type="array">
                <item name="apilog" xsi:type="object">Dhl\Paket\Model\Logger\Handler\Virtual</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Dhl\Paket\Webservice\Pipeline\Rate\RateResponseProcessor\Virtual" type="Dhl\ShippingCore\Webservice\Pipeline\Rate\RateResponseProcessor">
        <arguments>
            <argument name="processors" xsi:type="array">
                <item name="carrierDetails" xsi:type="object">Dhl\ShippingCore\Webservice\Pipeline\Rate\ResponseProcessor\CarrierDetails</item>
                <item name="freeShipping" xsi:type="object">Dhl\ShippingCore\Webservice\Pipeline\Rate\ResponseProcessor\FreeShipping</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Dhl\Paket\Model\Emulation\RateRequestService\Virtual" type="Dhl\ShippingCore\Model\Emulation\RateRequestService">
        <arguments>
            <argument name="responseProcessor" xsi:type="object">Dhl\Paket\Webservice\Pipeline\Rate\RateResponseProcessor\Virtual</argument>
        </arguments>
    </virtualType>
    <virtualType name="Dhl\Paket\Webservice\Pipeline\CreateShipments\ArtifactsContainerFactory\Virtual" type="\Dhl\ShippingCore\Api\Data\Pipeline\ArtifactsContainerInterfaceFactory">
        <arguments>
            <argument name="instanceName" xsi:type="string">Dhl\Paket\Webservice\Pipeline\CreateShipments\ArtifactsContainer</argument>
        </arguments>
    </virtualType>
    <virtualType name="Dhl\Paket\Webservice\Pipeline\Shipment\CreateShipmentsPipeline\Virtual" type="Dhl\ShippingCore\Webservice\Pipeline\Shipment\CreateShipmentsPipeline">
        <arguments>
            <argument name="stages" xsi:type="array">
                <item name="validate" sortOrder="100" xsi:type="object">Dhl\Paket\Webservice\Pipeline\CreateShipments\Stage\ValidateStage</item>
                <item name="mapRequest" sortOrder="200" xsi:type="object">Dhl\Paket\Webservice\Pipeline\CreateShipments\Stage\MapRequestStage</item>
                <item name="send" sortOrder="300" xsi:type="object">Dhl\Paket\Webservice\Pipeline\CreateShipments\Stage\SendRequestStage</item>
                <item name="mapResponse" sortOrder="400" xsi:type="object">Dhl\Paket\Webservice\Pipeline\CreateShipments\Stage\MapResponseStage</item>
            </argument>
            <argument name="artifactsContainerFactory" xsi:type="object">Dhl\Paket\Webservice\Pipeline\CreateShipments\ArtifactsContainerFactory\Virtual</argument>
        </arguments>
    </virtualType>
    <virtualType name="Dhl\Paket\Webservice\Pipeline\DeleteShipments\ArtifactsContainerFactory\Virtual" type="\Dhl\ShippingCore\Api\Data\Pipeline\ArtifactsContainerInterfaceFactory">
        <arguments>
            <argument name="instanceName" xsi:type="string">Dhl\Paket\Webservice\Pipeline\DeleteShipments\ArtifactsContainer</argument>
        </arguments>
    </virtualType>
    <virtualType name="Dhl\Paket\Webservice\Pipeline\Shipment\DeleteShipmentsPipeline\Virtual" type="Dhl\ShippingCore\Webservice\Pipeline\Track\RequestTracksPipeline">
        <arguments>
            <argument name="stages" xsi:type="array">
                <item name="mapRequest" sortOrder="100" xsi:type="object">Dhl\Paket\Webservice\Pipeline\DeleteShipments\Stage\MapRequestStage</item>
                <item name="send" sortOrder="200" xsi:type="object">Dhl\Paket\Webservice\Pipeline\DeleteShipments\Stage\SendRequestStage</item>
                <item name="mapResponse" sortOrder="300" xsi:type="object">Dhl\Paket\Webservice\Pipeline\DeleteShipments\Stage\MapResponseStage</item>
            </argument>
            <argument name="artifactsContainerFactory" xsi:type="object">Dhl\Paket\Webservice\Pipeline\DeleteShipments\ArtifactsContainerFactory\Virtual</argument>
        </arguments>
    </virtualType>
    <virtualType name="Dhl\Paket\Webservice\Pipeline\Shipment\DeleteShipmentsResponseProcessor\Virtual" type="Dhl\ShippingCore\Webservice\Pipeline\Track\TrackResponseProcessor">
        <arguments>
            <argument name="processors" xsi:type="array">
                <item name="updateLabelStatus" xsi:type="object">Dhl\Paket\Webservice\Pipeline\DeleteShipments\ResponseProcessor\UpdateLabelStatus</item>
                <item name="deleteTrack" xsi:type="object">Dhl\Paket\Webservice\Pipeline\DeleteShipments\ResponseProcessor\DeleteTrack</item>
                <item name="unsetShippingLabel" xsi:type="object">Dhl\Paket\Webservice\Pipeline\DeleteShipments\ResponseProcessor\UnsetShippingLabel</item>
            </argument>
        </arguments>
    </virtualType>

    <!-- # argument di overrides -->
    <type name="Dhl\Paket\Model\Carrier\Paket">
        <arguments>
            <argument name="ratesManagement" xsi:type="object">Dhl\Paket\Model\RatesManagement\Proxy</argument>
            <argument name="shipmentManagement" xsi:type="object">Dhl\Paket\Model\ShipmentManagement\Proxy</argument>
        </arguments>
    </type>
    <type name="Dhl\Paket\Model\RatesManagement">
        <arguments>
            <argument name="rateRequestService" xsi:type="object">Dhl\Paket\Model\Emulation\RateRequestService\Virtual</argument>
        </arguments>
    </type>
    <type name="Dhl\Paket\Model\ShipmentManagement">
        <arguments>
            <argument name="deleteResponseProcessor" xsi:type="object">Dhl\Paket\Webservice\Pipeline\Shipment\DeleteShipmentsResponseProcessor\Virtual</argument>
        </arguments>
    </type>
    <type name="Dhl\Paket\Model\BulkShipmentConfiguration">
        <arguments>
            <argument name="shipmentManagement" xsi:type="object">Dhl\Paket\Model\BulkShipmentManagement\Virtual</argument>
        </arguments>
    </type>
    <type name="Dhl\Paket\Webservice\ApiGateway">
        <arguments>
            <argument name="creationPipeline" xsi:type="object">Dhl\Paket\Webservice\Pipeline\Shipment\CreateShipmentsPipeline\Virtual</argument>
            <argument name="deletionPipeline" xsi:type="object">Dhl\Paket\Webservice\Pipeline\Shipment\DeleteShipmentsPipeline\Virtual</argument>
        </arguments>
    </type>
    <type name="Dhl\Paket\Webservice\ShipmentService">
        <arguments>
            <argument name="logger" xsi:type="object">Dhl\Paket\Model\Logger\Virtual</argument>
        </arguments>
    </type>
    <type name="Dhl\ShippingCore\Model\BulkShipmentManagement">
        <arguments>
            <argument name="configurations" xsi:type="array">
                <item name="dhlpaket" xsi:type="object">Dhl\Paket\Model\BulkShipmentConfiguration</item>
            </argument>
        </arguments>
    </type>
    <type name="Dhl\ShippingCore\Model\Checkout\CheckoutDataCompositeProcessor">
        <arguments>
            <argument name="processors" xsi:type="array">
                <item name="dhlpaketParcelManagementOptionsProcessor" xsi:type="object">Dhl\Paket\Model\Checkout\DataProcessor\ParcelManagementOptionsProcessor</item>
                <item name="dhlpaketParcelStationOptionsProcessor" xsi:type="object">Dhl\Paket\Model\Checkout\DataProcessor\ParcelStationOptionsProcessor</item>
            </argument>
        </arguments>
    </type>
    <type name="Dhl\ShippingCore\Model\LabelStatus\LabelStatusManagement">
        <arguments>
            <argument name="carrierCodes" xsi:type="array">
                <item name="dhlpaket" xsi:type="const">Dhl\Paket\Model\Carrier\Paket::CARRIER_CODE</item>
            </argument>
        </arguments>
    </type>
    <type name="Dhl\ShippingCore\Model\Packaging\PackagingDataCompositeProcessor">
        <arguments>
            <argument name="processors" xsi:type="array">
                <item name="dhlPaketServiceInputProcessor" sortOrder="120" xsi:type="object">Dhl\Paket\Model\Packaging\DataProcessor\ServiceInputDataProcessor</item>
                <item name="dhlPaketPackageInputProcessor" sortOrder="220" xsi:type="object">Dhl\Paket\Model\Packaging\DataProcessor\PackageInputDataProcessor</item>
            </argument>
        </arguments>
    </type>
    <type name="Dhl\ShippingCore\Model\Support\PackagingPopup">
        <arguments>
            <argument name="supportMap" xsi:type="array">
                <item name="dhlpaket" xsi:type="const">Dhl\Paket\Model\Carrier\Paket::CARRIER_CODE</item>
            </argument>
        </arguments>
    </type>
    <type name="Dhl\ShippingCore\Observer\DisableCodPaymentMethods">
        <arguments>
            <argument name="codSupportMap" xsi:type="array">
                <item name="dhlpaket" xsi:type="object">Dhl\Paket\Model\Carrier\CodSupportHandler</item>
            </argument>
        </arguments>
    </type>
    <type name="Dhl\ShippingCore\Observer\SplitAddress">
        <arguments>
            <argument name="carrierCodes" xsi:type="array">
                <item name="dhlpaket" xsi:type="const">Dhl\Paket\Model\Carrier\Paket::CARRIER_CODE</item>
            </argument>
        </arguments>
    </type>
</config>
